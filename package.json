{
  "name": "free-blog",
  "type": "module",
  "version": "0.0.2",
  "scripts": {
    "dev": "astro dev",
    "build": "npm run lint && npm run create-index && astro build",
    "preview": "npm run create-index && astro preview",
    "astro": "astro",
    "create-collection": "tsx scripts/createSchema.ts",
    "reset-template": "tsx scripts/reset_template.ts",
    "create-post": "tsx scripts/AI.ts && tsx scripts/create_index.ts",
    "get-routes": "tsx scripts/get_routes.ts",
    "create-multiple": "bash -c 'for col in \"$@\"; do npm run create-collection -- \"$col\"; done' --",
    "remove": "tsx scripts/remove.ts",
    "create-index": "tsx scripts/create_index.ts",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{js,ts,tsx,astro}\"",
    "check": "prettier --check \"**/*.{js,ts,tsx,astro}\""
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.0",
    "@astrojs/react": "^4.3.0",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.4.1",
    "@tailwindcss/vite": "^4.1.11",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "astro": "^5.10.2",
    "astro-icon": "^1.1.5",
    "fuse.js": "^7.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "sharp": "^0.34.2",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@iconify-json/mdi": "^1.2.3",
    "@tailwindcss/typography": "^0.5.16",
    "@types/glob": "^8.1.0",
    "@types/node": "^24.0.10",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "dotenv": "^17.0.1",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-astro": "^1.3.1",
    "glob": "^11.0.3",
    "gray-matter": "^4.0.3",
    "html-to-text": "^9.0.5",
    "inquirer": "^12.7.0",
    "openai": "^5.8.2",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "tsx": "^4.20.3",
    "zod": "^3.25.75"
  }
}
